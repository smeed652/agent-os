{
  "name": "hello-world-app",
  "version": "1.0.0",
  "description": "Agent-OS Hello World Test Application - Repeatable Development Process",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:performance": "jest tests/performance",
    "test:security": "jest tests/security",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write src/ tests/",
    "build": "node scripts/build.js",
    "reset": "node scripts/reset.js",
    "rebuild": "node scripts/rebuild.js",
    "validate": "node scripts/validate.js"
  },
  "keywords": [
    "hello-world",
    "express",
    "nodejs",
    "testing",
    "agent-os",
    "development-process"
  ],
  "author": "Agent-OS Development Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agent-os/hello-world-app.git"
  },
  "bugs": {
    "url": "https://github.com/agent-os/hello-world-app/issues"
  },
  "homepage": "https://github.com/agent-os/hello-world-app#readme"
}
